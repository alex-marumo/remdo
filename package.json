{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lexical/monorepo": "file:lexical",
    "@lexical/react": "file:lexical/packages/lexical-react",
    "@playwright/test": "^1.29.2",
    "@testing-library/react": "^13.3.0",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "playwright": "^1.29.2",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^4.0.3",
    "tiny-invariant": "^1.3.1",
    "web-vitals": "^2.1.4",
    "y-websocket": "^1.4.4",
    "yjs": "^13.5.41"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "server": "vite --host",
    "websocket": "HOST=0.0.0.0 PORT=8080 npx y-websocket",
    "build": "vite build",
    "eject": "react-scripts eject",
    "test-report": "npx playwright show-report --host 0.0.0.0",
    "test-browser": "VITE_DISABLECOLLAB=true npx playwright test --project=chromium",
    "test-collab": "npx playwright test --project=chromium",
    "test-ct": "playwright test -c playwright-ct.config.ts --project=chromium",
    "test-unit": "npx vitest run",
    "test-unit-watch": "npx vitest --ui",
    "test": "npm run test-unit && npm run test-ct && npm run test-browser"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.30.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/bootstrap": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "^0.28.4",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}
