{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hocuspocus/extension-logger": "^2.13.0",
    "@hocuspocus/provider": "^2.13.0",
    "@hocuspocus/server": "^2.13.0",
    "@lexical/list": "file:./lexical/packages/lexical-list",
    "@lexical/react": "file:./lexical/packages/lexical-react",
    "@lexical/utils": "file:./lexical/packages/lexical-utils",
    "@testing-library/react": "^15.0.7",
    "bootstrap": "git+https://github.com/twbs/bootstrap.git#v5.3.2",
    "bootstrap-icons": "^1.9.1",
    "envalid": "^8.0.0",
    "fs-extra": "^11.2.0",
    "lexical": "file:./lexical/packages/lexical",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "uuid": "^9.0.0",
    "web-vitals": "^4.0.0",
    "y-indexeddb": "^9.0.9",
    "y-websocket": "^2.0.3",
    "yjs": "^13.6.15"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "open": "./utils.sh open_document",
    "server": "VITE_DOCUMENTS=$(ls tests/data | sed 's/\\.json//' | tr '\n' ',') vite --host",
    "websocket-persist": "HOST=0.0.0.0 PORT=8080 YPERSISTENCE=./data/yjsDB npx y-websocket",
    "websocket": "HOST=0.0.0.0 PORT=8080 npx y-websocket",
    "hocuspocus": "node ./hocuspocus.mjs",
    "build": "vite build",
    "eject": "react-scripts eject",
    "test-browser": "VITE_DISABLECOLLAB=true npm run test-browser-collab",
    "test-browser-collab": "npx playwright test --project=chromium",
    "test-browser-watch": "VITE_DISABLECOLLAB=true ./utils.sh watch_repo 'npm run test-browser && echo \"\\x1b[32mDone, waiting for file changes...\\x1b[0m\"'",
    "test-unit": "VITE_DISABLECOLLAB=true npx vitest run",
    "test-performance": "VITE_PERFORMANCE_TESTS=true VITE_LOG_LEVEL=info time -f 'Time: %es' npx vitest run performance/performance --reporter ./tests/unit/performance/reporter.ts -t",
    "test-unit-collab": "npx vitest run",
    "test-unit-watch-coverage": "VITE_DISABLECOLLAB=true npx vitest --ui --coverage.enabled --coverage.include=src/**",
    "test-unit-watch": "VITE_DISABLECOLLAB=true npx vitest --watch --ui",
    "test": "npm run test-unit && npm run test-browser",
    "kill-npm-processes": "ps -eaf | grep 'npm run' | awk '{print $2}' |  xargs kill",
    "load": "./utils.sh run_serialization load",
    "save": "./utils.sh run_serialization save"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@microlink/react-json-view": "^1.23.0",
    "@playwright/test": "^1.37.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bootstrap": "^5.2.6",
    "@types/js-yaml": "^4.0.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "chokidar-cli": "^3.0.0",
    "esbuild": "^0.21.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-sort-json": "^2.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.59.2",
    "vite": "^5.2.11",
    "vitest": "^1.0.4",
    "vitest-preview": "^0.0.1"
  }
}
