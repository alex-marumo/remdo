{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lexical/list": "file:lexical",
    "@lexical/monorepo": "file:lexical",
    "@lexical/react": "file:lexical/packages/lexical-react",
    "@lexical/utils": "file:lexical",
    "@playwright/test": "^1.29.2",
    "@testing-library/react": "^13.3.0",
    "bootstrap": "git+https://github.com/twbs/bootstrap.git#v5.3.0-alpha1",
    "bootstrap-icons": "^1.9.1",
    "lexical": "file:lexical/packages/lexical",
    "playwright": "^1.29.2",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^4.0.3",
    "tiny-invariant": "^1.3.1",
    "web-vitals": "^2.1.4",
    "y-websocket": "^1.4.4",
    "yjs": "^13.5.41"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "server": "vite --host",
    "websocket": "HOST=0.0.0.0 PORT=8080 npx y-websocket",
    "build": "vite build",
    "eject": "react-scripts eject",
    "test-report": "npx playwright show-report data/playwright-report --host 0.0.0.0",
    "test-browser": "VITE_DISABLECOLLAB=true npm run test-browser-collab",
    "test-browser-collab": "npx playwright test --project=chromium | ./utils.sh trim_playwright",
    "test-browser-watch": "VITE_DISABLECOLLAB=true ./utils.sh watch_repo 'npm run test-browser'",
    "test-unit": "npx vitest run",
    "test-unit-watch": "VITE_DISABLECOLLAB=true npx vitest --ui",
    "test-unit-preview": "cd data && SERVER_MODE=vitest_preview PORT=3001 npx vitest-preview",
    "test": "npm run test-unit && npm run test-browser",
    "kill-npm-processes": "ps -eaf | grep 'npm run' | awk '{print $2}' |  xargs kill"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.30.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bootstrap": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "^0.28.4",
    "chokidar-cli": "^3.0.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.1",
    "prettier-plugin-sort-json": "^1.0.0",
    "vite": "^4.1.1",
    "vite-plugin-terminal": "^1.1.0",
    "vitest": "^0.28.4",
    "vitest-preview": "^0.0.1"
  }
}
